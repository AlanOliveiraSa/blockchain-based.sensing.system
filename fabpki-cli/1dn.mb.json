{
  "name":"mbblocknet_default",
  "description":"Connection profile to access the blockchain from the 1DN org",
  "version":"0.1",
  "client":{
    "organization":"1DN",
    "credentialStore":{
      "path":"/tmp/hfc-kvs",
      "cryptoStore":{
        "path":"/tmp/hfc-cvs"
      },
      "wallet":"wallet-name"
    }
  },
  "channels": {
    "1dn-channel": {
        "orderers": [
            "orderer.mb"
        ],
        "peers": {
            "peer0.1dn.mb": {
                "endorsingPeer": true,
                "chaincodeQuery": true,
                "ledgerQuery": true,
                "eventSource": true
            }
        }
    }
  },
  "organizations":{
    "orderer.mb":{
      "mspid": "OrdererMSP",
      "orderers": [
        "orderer.mb"
      ],
      "users": {
        "Admin": {
          "cert": "../crypto-config/ordererOrganizations/orderer.mb/users/Admin@orderer.mb/msp/signcerts/Admin@orderer.mb-cert.pem",
          "private_key": "../crypto-config/ordererOrganizations/orderer.mb/users/Admin@orderer.mb/msp/keystore/faf6325dbcb7029fa836126b3cbedb000a3cfeca730a17e60a7a47574f40b648_sk"}
      }
    },
    "1dn.mb":{
      "mspid": "1DNMSP",
      "peers":[
        "peer0.1dn.mb",
        "peer1.1dn.mb"
      ],
      "users": {
        "Admin": {
          "cert": "../crypto-config/peerOrganizations/1dn.mb/users/Admin@1dn.mb/msp/signcerts/Admin@1dn.mb-cert.pem",
          "private_key": "../crypto-config/peerOrganizations/1dn.mb/users/Admin@1dn.mb/msp/keystore/5db72b2015fb80cc244a3fb7d1bc71fc6b60c83321b6b86d20fe077d6b7affdc_sk"
        }
      }
    }
  },
  "orderers":{
    "orderer.mb":{
      "url":"192.168.0.12:7050",
      "grpcOptions": {
        "grpc.ssl_target_name_override": "orderer.mb",
        "grpc-max-send-message-length": 15
      },
      "tlsCACerts": {
        "path": "../crypto-config/ordererOrganizations/orderer.mb/tlsca/tlsca.orderer.mb-cert.pem"
      },
      "clientKey": {
        "path": "../crypto-config/ordererOrganizations/orderer.mb/orderers/orderer.mb.orderer.mb/tls/server.key"
      },
      "clientCert": {
        "path": "../crypto-config/ordererOrganizations/orderer.mb/orderers/orderer.mb.orderer.mb/tls/server.crt"
      }
    }
  },
  "peers":{
    "peer0.1dn.mb":{
      "url":"192.168.0.12:7051",
      "eventUrl":"192.168.0.12:7053",
      "grpcOptions": {
        "grpc.ssl_target_name_override": "peer0.1dn.mb",
        "grpc-max-send-message-length": 15
      },
      "tlsCACerts": {
        "path": "../crypto-config/peerOrganizations/1dn.mb/tlsca/tlsca.1dn.mb-cert.pem"
      },
      "clientKey": {
        "path": "../crypto-config/peerOrganizations/1dn.mb/peers/peer0.1dn.mb/tls/server.key"
      },
      "clientCert": {
        "path": "../crypto-config/peerOrganizations/1dn.mb/peers/peer0.1dn.mb/tls/server.crt"
      }
    }
  },
  "certificateAuthorities":{
    "ca.1dn.mb":{
      "url":"https://localhost:7054",
      "registrar":[
        {
          "enrollId":"admin",
          "enrollSecret":"adminpw"
        }
      ]
    }
  }
}
